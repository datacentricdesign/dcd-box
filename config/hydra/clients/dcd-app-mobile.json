{
  "client_id": "${APP_MOBILE_CLIENT_ID}",
  "name": "DCD - Mobile App Example",
  "description": "The HTTP API of the Data-Centric Design hub.",
  "client_secret": "${APP_DESIGNER_CLIENT_SECRET}",
  "scope": "openid,offline,profile,dcd:public,dcd:things,dcd:persons",
  "grant_types": ["authorization_code", "refresh_token"],
